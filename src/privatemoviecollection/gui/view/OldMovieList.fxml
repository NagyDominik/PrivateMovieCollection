<AnchorPane id="AnchorPane" prefHeight="412.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111" fx:controller="privatemoviecollection.gui.controller.OldMovieListController">
