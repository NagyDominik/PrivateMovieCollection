      <Button layoutX="221.0" layoutY="274.0" mnemonicParsing="false" onAction="#btnDeleteClick" prefHeight="31.0" prefWidth="158.0" text="Delete movie" />
      <Button layoutX="221.0" layoutY="321.0" mnemonicParsing="false" onAction="#btnDeleteAllClick" prefHeight="31.0" prefWidth="158.0" text="Delete all" />
