      <TextField fx:id="pathField" layoutX="160.0" layoutY="180.0" prefHeight="25.0" prefWidth="225.0" />
      <Label layoutX="15.0" layoutY="81.0" prefHeight="20.0" prefWidth="95.0" text="Title" textAlignment="RIGHT" AnchorPane.leftAnchor="15.0">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="15.0" layoutY="116.0" prefHeight="20.0" prefWidth="95.0" text="IMDb rating" AnchorPane.leftAnchor="15.0">
