<AnchorPane id="AnchorPane" prefHeight="558.0" prefWidth="530.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="privatemoviecollection.gui.controller.NewMovieController">
   <children>
      <TextField fx:id="titleField" layoutX="160.0" layoutY="75.0" prefHeight="25.0" prefWidth="225.0" />
