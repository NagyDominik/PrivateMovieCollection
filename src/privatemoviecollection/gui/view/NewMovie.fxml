      </Label>
      <Button fx:id="fileBtn" layoutX="390.0" layoutY="180.0" mnemonicParsing="false" onAction="#fileChooseClicked" prefHeight="25.0" prefWidth="90.0" text="File..." />
      <Button fx:id="saveBtn" layoutX="32.0" layoutY="292.0" mnemonicParsing="false" onAction="#saveClicked" prefHeight="25.0" prefWidth="90.0" text="Save" AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="15.0" />
