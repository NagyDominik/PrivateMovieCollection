            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="addCatToMovie" layoutX="253.0" layoutY="314.0" mnemonicParsing="false" onAction="#addCatToMovie" prefHeight="25.0" prefWidth="25.0" text="&lt;" AnchorPane.leftAnchor="253.0">
